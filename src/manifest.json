{
  "manifest_version": 2,
  "name": "dn-template-react-chrome-extension",
  "version": "0.0.1",
  "description": "基于react的chrome插件dawn模板",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "基于react的chrome插件dawn模板",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "storage",
    "webRequestBlocking",
    "browsingData",
    "<all_urls>"
  ],
  "homepage_url": "https://github.com"
}